{
  "name": "static-isomorphic-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for a NodeJS-Express backend and a React frontend.",
  "main": "server/app.js",
  "scripts": {
    "run:server:dev": "babel-node ./server/app.js --presets @babel/env --inspect",
    "run:server:prod": "webpack --config config/server/production/config.js",
    "build:client:dev:watch": "webpack --config config/client/development/config.js --watch",
    "build:client:prod": "webpack --config config/client/production/config.js",
    "start-dev": "run-p build:client:dev:watch run:server:dev",
    "build": "run-p build:client:prod run:server:prod",
    "start": "node ./builds/prod.build/server.prod.bundle.js",
    "tests": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BeeckmanThe1/static-isomorphic-boilerplate.git"
  },
  "author": "Thomas Opdebeeck",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BeeckmanThe1/static-isomorphic-boilerplate/issues"
  },
  "homepage": "https://github.com/BeeckmanThe1/static-isomorphic-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-proposal-throw-expressions": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-react": "^7.12.1",
    "@babel/runtime": "^7.14.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.2.4",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "open": "^8.0.7",
    "sass": "^1.32.12",
    "sass-loader": "^10.0.4",
    "style-loader": "^2.0.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
